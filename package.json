    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/platform-fastify": "^11.0.13",
    "@toss/nestjs-aop": "^2.1.6",
    "axios": "^1.8.4",
    "bcryptjs": "^3.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv-flow": "^4.1.0",
    "fastify": "^5.2.2",
    "ioredis": "^5.6.0",
    "joi": "^17.13.3",
    "mongoose": "^8.13.2",
    "nestjs-pino": "^4.4.0",
    "nodemailer": "^6.10.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-kakao": "^1.0.1",
    "passport-naver": "^1.0.6",
    "pino": "^9.6.0",
